{
    "rewrites": [
      {
        "source": "/webspatial/avp/assets/(.*)",
        "destination": "/webspatial/avp/assets/$1"
      },
      {
        "source": "/assets/(.*)",
        "destination": "/assets/$1"
      },
      {
        "source": "/icons/(.*)", 
        "destination": "/icons/$1"
      },
      {
        "source": "/manifest.json",
        "destination": "/manifest.json"
      },
      {
        "source": "/webspatial/avp/:path*",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }